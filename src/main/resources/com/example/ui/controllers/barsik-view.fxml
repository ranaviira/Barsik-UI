<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ui.controllers.BarsikController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <TableView fx:id="table" prefHeight="178.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="id" maxWidth="3000.0" prefWidth="59.0" text="id" />
            <TableColumn fx:id="name" prefWidth="139.0" text="name" />
            <TableColumn fx:id="login" prefWidth="129.0" text="login" />
            <TableColumn fx:id="password" prefWidth="165.0" text="password" />
        </columns>
    </TableView>
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
